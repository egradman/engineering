    "list": "bun run src/index.ts list",
    "cli:install": "bun run src/index.ts install",
    "test": "bun test"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "bun-types": "^1.0.0"
  },
  "pi": {
    "skills": ["./plugins/engineering/skills"],
    "prompts": [
      "./plugins/engineering/agents",
      "./plugins/engineering/commands"
