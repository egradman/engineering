{
  "name": "@every-env/compound-plugin",
  "version": "0.1.1",
  "type": "module",
  "private": false,
  "keywords": [
    "pi-package",
    "ai-powered",
    "engineering",
    "workflow-automation",
    "code-review"
  ],
  "bin": {
    "compound-plugin": "./src/index.ts"
  },
  "publishConfig": {
